


<p id="notice"><%= notice %></p>

<h1>Cvs</h1>

<table>
  <thead>
    <tr>
      <th>Path</th>
      <th>Student</th>
    </tr>
  </thead>

  <tbody>
    <% @cvs.each do |cv| %>
      <tr>
        <td><a href='<%=  cv.path %>'><%= "cv of "+cv.student.name %></a></td>
        <td><%= cv.student.name %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

